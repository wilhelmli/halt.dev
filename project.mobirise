\"0.1\" value=\"0\" condition=\"overlay && bg.type !== 'color'\">\n    <!-- End block parameters -->\n    </mbr-parameters> \n  <!--Overlay-->\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <!--Container-->\n<h1 mbr-if=\"showTitle\">文A</h1><br>\n<div class=\"container\">\n    <div id=\"carousel\">\n        <figure mbr-if=\"dimgslidercount > 0\"><img src=\"@PROJECT_PATH@/assets/images/yellow-172x129.png\" alt></figure>\n        <figure mbr-if=\"dimgslidercount > 1\"><img src=\"@PROJECT_PATH@/assets/images/green00ff00-172x129.png\" alt></figure>\n        <figure mbr-if=\"dimgslidercount > 2\"><img src=\"@PROJECT_PATH@/assets/images/orange-cirle-1-357x358.png\" alt></figure>\n        <figure mbr-if=\"dimgslidercount > 3\"><img src=\"@PROJECT_PATH@/assets/images/0000ffblue-172x129.png\" alt></figure>\n        <figure mbr-if=\"dimgslidercount > 4\"><img src=\"@PROJECT_PATH@/assets/images/greycccccc-172x129.png\" alt></figure>\n        <figure mbr-if=\"dimgslidercount > 5\"><img src=\"@PROJECT_PATH@/assets/images/whtffffff-172x129.png\" alt></figure>\n        <figure mbr-if=\"dimgslidercount > 6\"><img src=\"@PROJECT_PATH@/assets/images/orange-cirle-1-357x358.png\" alt></figure>\n        <figure mbr-if=\"dimgslidercount > 7\"><img src=\"@PROJECT_PATH@/assets/images/orange-cirle-1-357x358.png\" alt></figure>\n        <figure mbr-if=\"dimgslidercount > 8\"><img src=\"@PROJECT_PATH@/assets/images/orange-cirle-1-357x358.png\" alt></figure>\n    </div>\n</div>\n</section>",
